{
    "name": "hsc-co-works-dev-team/open-ai",
    "description": "OpenAI GPT-3 Api Client in PHP >= 7.3",
    "keywords": [
        "open-ai-php73",
        "open-ai"
    ],
    "homepage": "https://github.com/HSC-CoWorks-Dev-Team/open-ai",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Orhanerday\\OpenAi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orhanerday\\OpenAi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
